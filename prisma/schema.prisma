generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dispenser {
  id          String    @id @default(uuid())
  opened_at   DateTime?
  closed_at   DateTime?
  flow_volume Float
  total_spent Float     @default(0.0000)
}
